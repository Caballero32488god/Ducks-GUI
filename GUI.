local button = script.Parent
local soundId = "rbxassetid://102202479368253" -- Your music asset ID

local function changeMusic()
    -- Create a new Sound object
    local sound = Instance.new("Sound")
    sound.SoundId = soundId
    sound.Parent = game.Workspace -- You can change this to a different location if needed
    sound:Play()
end

button.MouseButton1Click:Connect(changeMusic)
