local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local DropdownKillMultiple = Instance.new("Frame") -- Placeholder for dropdown
local KillMultipleButton = Instance.new("TextButton")

-- GUI setup
ScreenGui.Parent = game.CoreGui
Frame.Parent = ScreenGui
Frame.Size = UDim2.new(0, 300, 0, 200)
Frame.Position = UDim2.new(0.5, -150, 0.5, -100)
Frame.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)

-- Dropdown setup (simplified)
DropdownKillMultiple.Parent = Frame
DropdownKillMultiple.Size = UDim2.new(1, 0, 0, 40)
DropdownKillMultiple.BackgroundColor3 = Color3.new(0.4, 0.4, 0.4)
DropdownKillMultiple.Text = "Kill Multiple People (Not Interactive)"

-- Button setup
KillMultipleButton.Parent = Frame
KillMultipleButton.Size = UDim2.new(1, 0, 0, 40)
KillMultipleButton.Position = UDim2.new(0, 0, 0, 50)
KillMultipleButton.BackgroundColor3 = Color3.new(0.3, 0.5, 0.3)
KillMultipleButton.Text = "Update Dropdown"

KillMultipleButton.MouseButton1Click:Connect(function()
    local playerNames = {}
    for _, player in ipairs(Players:GetPlayers()) do
        table.insert(playerNames, player.Name)
    end
    
    -- Here, you'd replace the placeholder text with player names
    DropdownKillMultiple.Text = "Players: " .. table.concat(playerNames, ", ")
end)

-- Initial population of the dropdown
local function updateKillMultipleDropdown()
    local playerNames = {}
    for _, player in ipairs(Players:GetPlayers()) do
        table.insert(playerNames, player.Name)
    end

    -- Here, you'd replace the placeholder text with player names
    DropdownKillMultiple.Text = "Players: " .. table.concat(playerNames, ", ")
end

-- Update when a player joins or leaves
Players.PlayerAdded:Connect(updateKillMultipleDropdown)
Players.PlayerRemoving:Connect(updateKillMultipleDropdown)

-- Initial load
updateKillMultipleDropdown()
