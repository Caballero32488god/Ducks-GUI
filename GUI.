local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.CoreGui

-- Create Main Frame
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0.5, 0, 0.5, 0)
mainFrame.Position = UDim2.new(0.25, 0, 0.25, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
mainFrame.Parent = ScreenGui

-- Function to create a new tab
local function createTab(name)
    local tab = Instance.new("Frame")
    tab.Name = name
    tab.Size = UDim2.new(1, 0, 1, 0)
    tab.Visible = false
    tab.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
    tab.Parent = mainFrame
    return tab
end

-- Create Tabs
local mainTab = createTab("Main")
local killerTab = createTab("Killer")
local autoFarmTab = createTab("AutoFarm")
local teleportTab = createTab("Teleport")
local settingsTab = createTab("Settings")
local statsTab = createTab("Stats")

-- Function to switch between tabs
local function switchTab(selectedTab)
    mainTab.Visible = false
    killerTab.Visible = false
    autoFarmTab.Visible = false
    teleportTab.Visible = false
    settingsTab.Visible = false
    statsTab.Visible = false
    selectedTab.Visible = true
end

-- Create Tab Buttons
local tabButtons = {}
local function createTabButton(name)
    local button = Instance.new("TextButton")
    button.Text = name
    button.Size = UDim2.new(0.2, 0, 0.1, 0)
    button.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    button.Parent = mainFrame
    button.MouseButton1Click:Connect(function() switchTab(createTab(name)) end)
    return button
end

-- Adding tab buttons
tabButtons.Main = createTabButton("Main")
tabButtons.Killer = createTabButton("Killer")
tabButtons.AutoFarm = createTabButton("AutoFarm")
tabButtons.Teleport = createTabButton("Teleport")
tabButtons.Settings = createTabButton("Settings")
tabButtons.Stats = createTabButton("Stats")

-- Default tab
switchTab(mainTab)

-- Main Tab Buttons
local speedButton = Instance.new("TextButton", mainTab)
speedButton.Text = "Speed Changer"
speedButton.Size = UDim2.new(0, 200, 0, 50)
speedButton.Position = UDim2.new(0.5, -100, 0.1, 0)
speedButton.MouseButton1Click:Connect(function()
    -- Speed changing logic here
end)

-- Killer Tab Buttons
local autoKillButton = Instance.new("TextButton", killerTab)
autoKillButton.Text = "Auto Kill"
autoKillButton.Size = UDim2.new(0, 200, 0, 50)
autoKillButton.Position = UDim2.new(0.5, -100, 0.1, 0)
autoKillButton.MouseButton1Click:Connect(function()
    -- Auto Kill logic here
end)

-- Teleport Tab
local teleportDropdown = Instance.new("TextButton", teleportTab)
teleportDropdown.Text = "Teleport to Player"
teleportDropdown.Size = UDim2.new(0, 200, 0, 50)
teleportDropdown.Position = UDim2.new(0.5, -100, 0.1, 0)

teleportDropdown.MouseButton1Click:Connect(function()
    local playerName = "PlayerName" -- Replace with input for actual player name
    local targetPlayer = Players:FindFirstChild(playerName)
    if targetPlayer and targetPlayer.Character then
        LocalPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
    end
end)

-- Settings Tab
local toggleFPSButton = Instance.new("TextButton", settingsTab)
toggleFPSButton.Text = "Toggle FPS Display"
toggleFPSButton.Size = UDim2.new(0, 200, 0, 50)
toggleFPSButton.Position = UDim2.new(0.5, -100, 0.1, 0)

toggleFPSButton.MouseButton1Click:Connect(function()
    -- Toggle FPS logic here
end)

-- Stats Tab
local statsInput = Instance.new("TextBox", statsTab)
statsInput.Size = UDim2.new(0.9, 0, 0.1, 0)
statsInput.Position = UDim2.new(0.05, 0, 0.1, 0)
statsInput.PlaceholderText = "Enter Username"

local getStatsButton = Instance.new("TextButton", statsTab)
getStatsButton.Text = "Get Stats"
getStatsButton.Size = UDim2.new(0.9, 0, 0.1, 0)
getStatsButton.Position = UDim2.new(0.05, 0, 0.3, 0)

getStatsButton.MouseButton1Click:Connect(function()
    local username = statsInput.Text
    -- Function to get player stats
    local targetPlayer = Players:FindFirstChild(username)
    if targetPlayer then
        local leaderstats = targetPlayer:FindFirstChild("leaderstats")
        if leaderstats then
            -- Display stats logic here
        else
            print("Leaderstats not found.")
        end
    else
        print("Player not found.")
    end
end)

-- Initialize
print("GUI initialized successfully!")
