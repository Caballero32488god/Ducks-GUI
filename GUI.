local HorizonLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/6ayy/HorizonLibrary/main/Library.lua"))()
local Players = game:GetService("Players")

local Window = HorizonLibrary:CreateWindow({
    Name = "Game Utility Script",
    Version = "1.2"
})

local ConsoleTab = Window:CreateConsoleTab({
    Name = "Console",
    Icon = "rbxassetid://14290789640",
    FirstWindow = true
})

local function updateStatus(player)
    local stats = player:FindFirstChild("leaderstats")
    if stats then
        local statuses = {
            Strength = stats:FindFirstChild("Strength"),
            Durability = stats:FindFirstChild("Durability"),
            Agility = stats:FindFirstChild("Agility"),
            Rebirths = stats:FindFirstChild("Rebirths"),
            Brawls = stats:FindFirstChild("Brawls"),
            EvilKarma = stats:FindFirstChild("Evil Karma"),
            GoodKarma = stats:FindFirstChild("Good Karma"),
            TotalKills = stats:FindFirstChild("Total Kills"),
            Gems = stats:FindFirstChild("Gems"),
        }
        
        for key, value in pairs(statuses) do
            if value then
                ConsoleTab:CreateConsoleAlert({
                    RobberyLocation = key,
                    RobberyStatus = "Status - " .. tostring(value.Value)
                })
                value.Changed:Connect(function()
                    ConsoleTab:CreateConsoleAlert({
                        RobberyLocation = key,
                        RobberyStatus = "Status - " .. tostring(value.Value)
                    })
                end)
            end
        end
    end
end

Players.PlayerAdded:Connect(function(player)
    player:WaitForChild("leaderstats")
    updateStatus(player)
    player.leaderstats.ChildAdded:Connect(function(child)
        updateStatus(player)
    end)
end)

for _, player in Players:GetPlayers() do
    updateStatus(player)
end

