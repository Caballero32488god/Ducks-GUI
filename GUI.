local HorizonLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/6ayy/HorizonLibrary/main/Library.lua"))()
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local Window = HorizonLibrary:CreateWindow({
    Name = "Ducks are cool",
    Version = "1.2"
})

local ConsoleTab = Window:CreateConsoleTab({
    Name = "Console",
    Icon = "rbxassetid://14290789640",
    FirstWindow = true
})

local function updateStats()
    local stats = LocalPlayer:FindFirstChild("leaderstats")
    if stats then
        local statNames = {
            "Strength", "Durability", "Agility", "Rebirths", 
            "Brawls", "Evil Karma", "Good Karma", "Total Kills", "Gems"
        }
        for _, statName in ipairs(statNames) do
            local stat = stats:FindFirstChild(statName)
            if stat and stat:IsA("IntValue") then
                ConsoleTab:CreateConsoleAlert({
                    RobberyLocation = statName,
                    RobberyStatus = "Status - " .. tostring(stat.Value)
                })
                stat.Changed:Connect(function()
                    ConsoleTab:CreateConsoleAlert({
                        RobberyLocation = statName,
                        RobberyStatus = "Status - " .. tostring(stat.Value)
                    })
                end)
            end
        end
    end
end

-- Update stats when the player's character is added
LocalPlayer.CharacterAdded:Connect(function()
    wait(1) -- Wait for leaderstats to load
    updateStats()
end)

-- Initial stats update
updateStats()

