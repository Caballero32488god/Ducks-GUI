do
    -- Example Button in Main
    Tabs.Main:AddButton({
        Title = "Example Button",
        Callback = function()
            print("Example Button clicked!")
        end
    })

    -- Dropdowns in Kill
    local playerList = {}

    for _, player in pairs(game.Players:GetPlayers()) do
        table.insert(playerList, player.Name)
    end

    Tabs.Kill:AddDropdown("SingleKill", {
        Title = "Select a Player to Kill",
        Values = playerList,
        Multi = false,
        Default = playerList[1],
    }):OnChanged(function(selectedPlayer)
        print("Selected player to kill:", selectedPlayer)
        -- Add kill logic here
    end)

    Tabs.Kill:AddDropdown("MultipleKill", {
        Title = "Select Players to Kill",
        Values = playerList,
        Multi = true,
        Default = {},
    }):OnChanged(function(selectedPlayers)
        print("Selected players to kill:", table.concat(selectedPlayers, ", "))
        -- Add kill logic here
    end)

    -- Dropdown in Stats
    Tabs.Stats:AddDropdown("SelectPlayerStats", {
        Title = "Select Player",
        Values = playerList,
        Multi = false,
        Default = playerList[1],
    }):OnChanged(function(selectedPlayer)
        print("Showing stats for:", selectedPlayer)
        -- Add logic to display stats here
    end)

    -- Dropdown for GUI Color in Settings
    local colorOptions = {}
    for themeName in pairs(Themes) do
        table.insert(colorOptions, themeName)
    end

    Tabs.Settings:AddDropdown("SelectTheme", {
        Title = "Select Theme",
        Values = colorOptions,
        Multi = false,
        Default = "Darker",
    }):OnChanged(function(selectedTheme)
        if Themes[selectedTheme] then
            Window:SetTheme(Themes[selectedTheme])
        end
    end)

end
