local HorizonLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/6ayy/HorizonLibrary/main/Library.lua"))()
local Window = HorizonLibrary:CreateWindow({
    Name = "Ducks are cool",
    Version = "1.2"
})

-- Create a Settings Tab
local SettingsTab = Window:CreateSettingsTab({
    Name = "Settings",
    Icon = "rbxassetid://14290827572",
    FirstWindow = true
})

-- Movement Section
local MovementSection = SettingsTab:CreateSection({
    Name = "Movement"
})

MovementSection:CreateButton("No Hit Speed", function()
    -- Functionality for No Hit Speed
end)

MovementSection:CreateButton("Kill", function()
    -- Functionality for Kill
end)

local isLocked = false
local Player = game.Players.LocalPlayer

MovementSection:CreateButton("Lock Position", function()
    isLocked = not isLocked
    
    if isLocked then
        Player.Character.HumanoidRootPart.Anchored = true
        Player.Character.Humanoid.AutoRotate = false
        print("Position Locked")
    else
        Player.Character.HumanoidRootPart.Anchored = false
        Player.Character.Humanoid.AutoRotate = true
        print("Position Unlocked")
    end
end)

MovementSection:CreateButton("Time Zone", function()
    -- Functionality for Time Zone
end)

-- Speed Controls Section
local SpeedSection = SettingsTab:CreateSection({
    Name = "Speed Controls"
})

SpeedSection:CreateTextBox("Set Speed", "Change numbers by typing it", true, function(Value)
    local speed = tonumber(Value)
    if speed then
        Player.Character.Humanoid.WalkSpeed = speed
    end
end)

SpeedSection:CreateButton("Walk Water (Coming Soon)", function()
    -- Functionality for Walk Water
end)

SpeedSection:CreateButton("FPS", function()
    _G.Ignore = {}
    _G.Settings = {
        Players = {
            ["Ignore Me"] = true,
            ["Ignore Others"] = true,
            ["Ignore Tools"] = true
        },
        Other = {
            ["FPS Cap"] = 360,
            ["No Camera Effects"] = true,
            ["No Clothes"] = true,
            ["Low Water Graphics"] = true,
            ["No Shadows"] = true,
            ["Low Rendering"] = true,
            ["Low Quality Parts"] = true,
            ["Low Quality Models"] = true,
            ["Reset Materials"] = true,
        }
    }
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CasperFlyModz/discord.gg-rips/main/FPSBooster.lua"))()
end)

local infiniteJumpEnabled = false

SpeedSection:CreateButton("Infinite Jump", function()
    local UserInputService = game:GetService("UserInputService")
    
    infiniteJumpEnabled = not infiniteJumpEnabled

    if infiniteJumpEnabled then
        UserInputService.JumpRequest:Connect(function()
            if infiniteJumpEnabled and Player.Character then
                Player.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
            end
        end)
        print("Infinite Jump Enabled")
    else
        print("Infinite Jump Disabled")
    end
end)

local antiKnockbackEnabled = false

SpeedSection:CreateButton("Anti Knockback", function()
    antiKnockbackEnabled = not antiKnockbackEnabled
    
    if antiKnockbackEnabled then
        local character = Player.Character or Player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        
        humanoid:GetPropertyChangedSignal("PlatformStand"):Connect(function()
            if antiKnockbackEnabled then
                humanoid.PlatformStand = true
            end
        end)

        print("Anti Knockback Enabled")
    else
        Player.Character:FindFirstChildOfClass("Humanoid").PlatformStand = false
        print("Anti Knockback Disabled")
    end
end)

