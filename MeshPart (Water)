local function changeSeaPartsToCyanBlocks()
    -- Iterate through all parts in the workspace
    for _, part in pairs(workspace:GetDescendants()) do
        if part:IsA("SeaPart") then
            -- Create a new Part
            local cyanBlock = Instance.new("Part")
            cyanBlock.Size = part.Size
            cyanBlock.Color = Color3.fromRGB(0, 255, 255) -- Cyan color
            cyanBlock.Position = part.Position
            cyanBlock.Anchored = false -- Make it walkable
            cyanBlock.CanCollide = true
            cyanBlock.Material = Enum.Material.SmoothPlastic -- Optional: adjust material

            -- Parent the new Part to the workspace
            cyanBlock.Parent = workspace

            -- Optionally, you can remove the original SeaPart
            part:Destroy() -- Comment this line if you want to keep the original SeaPart
        end
    end
end

-- Call the function to change SeaParts to cyan blocks
changeSeaPartsToCyanBlocks()
